.fixed-date{
   position: fixed; 
   left: 0;
   right: 0;
   top:0;
   background: #fff;
   height: 240rpx; 
}
.current-date{
  width: 91%;
  margin: 0 auto;
  padding-top: 10rpx;
}
.month{
  color: #FF604B;
  font-size: 30rpx;
}
.year{
  margin-left: 10rpx;
  font-size: 28rpx;
  color: gray;
}
.klass-date{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 20rpx;
}
.klass-day{
  width:100%;
  height: 56rpx;
  margin: 20rpx auto auto auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.klass-count{
  position: relative;
  top:-42rpx;
  left: 84rpx; 
}
.klasses{
  width:100%;
  margin-top: 300rpx;
}
.klass{
  width:100%;
  height: 300rpx; 
  margin-top: 30rpx;
}
.klass_time{
 margin-left: 7%;
 padding-top: 40rpx;
 color: #656565;
 font-size: 28rpx;
}
.klass-dayLook{
  width: 14.22%;
  float: left;
  text-align: center;
}
.active{
  display: block; 
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  line-height: 45rpx;
  text-align: center;
  margin: 0 auto;
  background: #FF604B;
  color: #fff;
}
.active1{
  display: block; 
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  line-height: 45rpx;
  text-align: center;
  margin: 0 auto;
  background: #000;
  color: #fff;
}
#isChange{
  display: block; 
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  line-height: 56rpx;
  text-align: center;
  margin: 0 auto;
  background: #FF604B;
  color: #fff;
}
