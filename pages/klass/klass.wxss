  align-items: center;
  justify-content: space-around;
}
.student_avatar{
  width: 120rpx;
  height: 120rpx;
  margin-left: 5%;
}
.student_avatar>image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.student_klass{
  width: 72%;
}
.student_name{
  margin-top: 15rpx;
  font-size: 28rpx;
}
.klass_name{
  font-size: 30rpx;
  font-weight: 900;
}
.log-list {
  display: flex;
  flex-direction: column;
  margin-top: 55rpx;  
}
.log-item {
  margin: 10rpx;
}
.personal_header{
  width: 100%;
  height: 398rpx;
  margin-top: 70rpx;
}
