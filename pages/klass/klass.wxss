.catalog_item>text{
  color: #000;
  position: relative;
  top:-73rpx;
  left:0;
}
.week{
 font-size: 28rpx; 
}
.catalog_item, .count{
  font-size: 28rpx;
}
.count{
  width: 14%;
  float: left;
  color: #FF604B;
}
.klass_title{
  margin-top: 25rpx;
